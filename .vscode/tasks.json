{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Build WordNet.Wpf",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/WordNet.Wpf/WordNet.Wpf.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile",
            "options": {
                "env": {
                    "SolutionDir": "${workspaceFolder}"
                }
            },
        },
        {
            "label": "Build WordNet.Import",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/WordNet.Import/WordNet.Import.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile",
        },
    ]
}